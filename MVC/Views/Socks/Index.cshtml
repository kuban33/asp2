@model IEnumerable<Socks>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@

@{
    DateTime datum = (DateTime)ViewData["datum"];
}

<h1>Ponozky</h1>

<div>
    @datum.ToString("yyyyMMdd")
</div>

@foreach(var sock in Model)
{
    <p>
        <a href="/socks/getbyid/@sock.Id">@sock.Id</a> - @sock.Brand - @sock.Size - @sock.Price - @sock.OnStock
    </p>
}
